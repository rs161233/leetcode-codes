import java.util.*;
class Main {
    public static void main(String[] args) {
        String str="akashkgkuy fuiqkefguesh";
        
        System.out.println("countOccurance :"+ countOccurance(str.toLowerCase()));
        
    }   
        
    public static String countOccurance(String str){
       Map<Character, Integer> occuranceMap= new HashMap<>();
       for(char c: str.toCharArray()){
           occuranceMap.put(c,occuranceMap.getOrDefault(c,0)+1);
       }
       return occuranceMap.toString();
    }
    
}
